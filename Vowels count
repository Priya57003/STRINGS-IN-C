#include <stdio.h>
#include <string.h>
int main()
{
   char str1[100];
   scanf("%s",str1);
   int count1=0,count2=0,count3=0;
   for(int i=0;i<strlen(str1);i++)
   {
       if(str1[i]=='a'||str1[i]=='e'||str1[i]=='i'||str1[i]=='o'||str1[i]=='u'||str1[i]=='A'||str1[i]=='E'||str1[i]=='I'||str1[i]=='O'||
       str1[i]=='U')
       {
           count1++;
       }
       else 
       {
           count2++;
       }
   }
   printf("NUM OF VOWELS AND CONSONANTS IN STRING GOWTHAMPRIYA %d %d"+count1+count2);

   
    return 0;
}
